{
  "name": "vscode-context-menu-structure",
  "displayName": "vscode-context-menu-structure",
  "description": "Ð¡ustom organization of context menu items",
  "version": "0.1.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "context-menu.runInTerminal",
        "title": "%context-menu.runInTerminal.title%",
        "category": "1_myPython"
      },
      {
        "command": "context-menu.runTask",
        "title": "%context-menu.runTask.title%",
        "group": "1_myPython"
      },
      {
        "command": "context-menu.runPytest",
        "title": "%context-menu.runPytest.title%",
        "category": "1_myPython"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "context-menu.runInTerminal",
          "when": "editorLangId == python",
          "group": "1_myPython@0"
        },
        {
          "command": "context-menu.runTask",
          "when": "editorLangId == python",
          "group": "1_myPython@2"
        },
        {
          "command": "context-menu.runPytest",
          "when": "editorLangId == python",
          "group": "1_myPython@3"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.23.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}